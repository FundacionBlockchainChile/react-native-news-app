{
  "expo": {
    "name": "Hacker News App",
    "slug": "HackerNewsApp",
    "privacy": "public",
    "description": "List latest Hacker News and Read it inside the App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sergiocastro.newsapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
  }
}